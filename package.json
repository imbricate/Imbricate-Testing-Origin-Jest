{
  "name": "@imbricate/test-origin-jest",
  "packageManager": "yarn@4.4.0",
  "version": "2.3.0",
  "description": "Jest test kit for Imbricate Origin Testing",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "build": "tsc --project typescript/tsconfig.build.json",
    "license": "sdb license",
    "release": "yarn lint && yarn license && yarn build",
    "publish-dry-run": "yarn release && sdb --only publish-dry-run",
    "publish": "yarn release && sdb --only publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Imbricate/Imbricate-Testing-Origin-Jest.git"
  },
  "keywords": [
    "API"
  ],
  "author": "WMXPY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Imbricate/Imbricate-Testing-Origin-Jest/issues"
  },
  "homepage": "https://imbricate.io",
  "peerDependencies": {
    "@imbricate/core": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@imbricate/core": "^2.6.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}